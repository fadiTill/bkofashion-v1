hion-v1/src/reportWebVitals.js",[],["95","96"],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/App.js",["97","98"],"import Checkout from \"./Checkout.js\"\nimport Header from './Header.js';\nimport Payment from './Payment.js';\nimport Home from './Home.js';\nimport Login from './Login.js';\nimport Productdetails from './Productdetails.js';\nimport './App.css';\nimport {BrowserRouter as Router, Switch, Route}\nfrom \"react-router-dom\";\nimport {auth} from \"./firebase\";\nimport {useStateValue} from \"./StateProvider\";\nimport React, {useEffect} from 'react';\n\n\n\n\n\n\nfunction App() {\n  const[{}, dispatch]= useStateValue();\n//  similar to componentdidMount and componentDidUpdate\n  useEffect(() => {\n\nauth.onAuthStateChanged((authUser) => {\n  console.log(\"the  User is >>>\", authUser);\n\n  if (authUser) {\n    \n    dispatch({\n      type:\"SET_USER\",\n      user: authUser,\n    });\n\n  } else {\n    dispatch({\n      type:\"SET_USER\",\n      user: null,\n    })\n  \n  }\n\n});\n  }, []);\n  return (\n    //BEM\n    <Router>\n    <div className=\"App\">\n      <Switch>\n       <Route path=\"/login\">\n       <h1>login</h1>\n       <Login/>\n      </Route>\n      <Route path=\"/productdetails\">\n       <Header/>\n       <Productdetails/>\n      </Route>\n      <Route path=\"/checkout\">\n      <Header/>\n      <Checkout/>\n      </Route>\n      <Route path=\"/payment\">\n      <Header/>\n      <Payment/>\n      </Route>\n       <Route path=\"/\">\n       <Header/>\n      <Home/>\n      </Route>\n      </Switch>\n    </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/index.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Header.js",["99"],"import React from 'react'\nimport './Header.css'\nimport './Productdetails.css'\nimport  SearchIcon  from \"@material-ui/icons/Search\";\nimport ShoppingBasketIcon from '@material-ui/icons/ShoppingBasket';\nimport {Link} from \"react-router-dom\"\nimport {useStateValue} from \"./StateProvider\";\nimport { auth } from './firebase';\n\nfunction Header() {\n  const [{basket, user}, dispatch] = useStateValue();\n\n  const handleAuthenticaton = () => {\n    if (user){\n      auth.signOut();\n    }\n  }\n    return (\n        <div className=\"header\">\n          <Link to=\"/\" >\n          <img className=\"header_logo\"src=\"images/transparent.png\" alt=\"logo\"/> \n          </Link>\n        <div className=\"header_search\">\n          <input className=\"header_searchInput\" type=\"text\"/>\n          <SearchIcon\n           className=\"header_searchIcon\" />\n        </div>\n\n        <div className=\"header_nav\">\n          <Link to={!user &&\"/login\"}>\n        <div  onClick={handleAuthenticaton} className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Hello {!user? 'Guest' : user.email}</span>\n       <span\n        className=\"header_optionLineTwo\">{user? 'sign Out':'Sign In'}</span>\n       </div>\n       </Link>\n\n       <div className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Returns</span>\n       <span\n        className=\"header_optionLineTwo\">& Orders</span>\n       </div>\n\n       <div className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Your</span>\n       <span\n        className=\"header_optionLineTwo\">BKOPSales </span>\n       </div>\n       \n       <div className=\"header_nav\">\n       \n       <Link to=\"/productdetails\">\n       <div  className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Product</span>\n       <span\n        className=\"header_optionLineTwo\">Details</span>\n        </div>\n        </Link>\n       </div>\n      \n      <Link to=\"/checkout\">\n      <div  className=\"header_optionCart\">\n      <ShoppingBasketIcon/>\n    <span className=\"header_optionLineTwo header_basketCount\">{basket?.length}</span>\n      </div>\n      </Link>\n   \n   </div>\n  </div>\n    )\n};\n\nexport default Header\n\n\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Home.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Product.js",["100"],"\n\nimport React from 'react';\nimport './Product.css';\nimport {useStateValue} from \"./StateProvider\";\n\nfunction Product({id, title, image, price, rating}) {\n     const [{basket}, dispatch] = useStateValue();\n    //  console.log(\" my basket>>\", basket);\n\n    const addBasket = () => {\n        // dispatch action into the data layer\n        dispatch({\n            type: \"ADD_BASKET\",\n            item: {\n            id: id,\n            title: title,\n            price: price,\n            rating: rating,\n            image: image,\n            },\n        });\n    };\n    return (\n        <div className=\"product\">\n            <div className=\"product-info\">\n                <p>{title}</p>\n                <p className=\"product-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n                <div className=\"product-rating\">\n                {Array(rating).fill().map ((_, i) => (<p>★</p> ))}\n                </div>\n            </div>\n                <img  className=\"product-image\" src={image}  alt=\"\"/>\n                <button onClick={addBasket}> Add to basket</button>\n            </div>\n       \n        \n    )\n}\n\nexport default Product\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Checkout.js",["101","102"],"\n\nimport React from 'react';\nimport  \"./Checkout.css\";\nimport Total from \"./Total.js\";\nimport Payment from \"./Payment.js\";\nimport { useStateValue} from \"./StateProvider\";\n// import { BasketItems } from './Reducer';\n import CheckoutItem from './CheckoutItem';\nimport { useHistory } from 'react-router-dom';\n\n\nfunction Checkout() {\n    const History = useHistory();\n    const [{basket}, dispatch] = useStateValue();\n    return (\n        <div className=\"checkout\">\n            <div clasName=\"chechout-left\">\n           {/* <img className=\"checkout-img\"src=\"images/image-top.jpg\" alt=\"\"/>  */}\n        \n           {/* <div>  */}\n\n           \n               <h2 className=\"checkout-title\"> Your shoping basket</h2>\n\n\n               {basket.map (item => (\n            <CheckoutItem\n            id={item.id} \n            title= {item.title}\n            price= {item.price}\n            rating= {item.rating}\n            image={item.image}\n            />\n               ))}\n               {/* </div> */}\n        </div>\n        <div  onClick={event => History.push('/payment')}className=\"checkout-right\">\n            <Total/>\n\n        </div>\n    </div>\n\n        \n\n       \n    );\n}\n\nexport default Checkout\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/total.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Total.js",["103"],"import React from 'react';\nimport   \"./Total.css\";\nimport CurrencyFormat from \"react-currency-format\";\nimport{BasketItems} from \"./Reducer\";\nimport {useStateValue} from \"./StateProvider\";\nimport CheckoutTotal from './CheckoutTotal';\n\n\n\nfunction Total() {\n    const [{basket}, dispatch]= useStateValue();\n    \n    return (\n       <div>\n        <div className=\"subtotal\">\n           <CurrencyFormat\n          renderText={(value)=> (\n              <> \n              <p>\n                  detail({basket.length} items):\n         \n          {basket.map (item => (\n            <CheckoutTotal\n            id={item.id} \n            title= {item.title}\n            price= {item.price}\n            \n            />\n               ))}\n               <hr></hr>\n                <strong> Total:{value}</strong>\n              </p>\n              {/* <small className=\"subtotal-gift\">\n                  <input type=\"checkbox\"/>This order\n                  contains a gift\n              </small> */}\n             \n                  </>\n          )}\n          decimalScale={2}\n          value={BasketItems(basket)}\n          displayType={\"text\"}\n          thousandSeparator={true}\n          prefix={\"$\"}\n          /> \n          <button> Proceed to checkout</button>\n        </div>\n        </div>\n    )\n}\n\nexport default Total\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/StateProvider.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Reducer.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/CheckoutItem.js",["104"],"import React from 'react'\nimport { useStateValue} from \"./StateProvider\";\nimport \"./CheckoutItem.css\"\n\nfunction CheckoutItem({id, title, image, price, rating}) {\n    const [{basket}, dispatch] = useStateValue();\n\n    const deleteBasket =()=> {\n        dispatch({\n            type: 'DELETE_BASKET',\n            id: id,\n        })\n    }\n    \n    return (\n        <div className=\"checkoutitems\">\n             \n           <div className=\"checkoutitems-info\">\n           \n               <p className=\"checkoutitme_title\">{title}</p>\n               <img  className=\"checkoutitems-image\" src={image}  alt=\"\"/>\n\n\n                <p className=\"checkoutitems-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n                <div className=\"checkoutitems-rating\">\n                {Array(rating).fill().map ((_, i) => (<p>★</p> ))}\n                </div>\n           \n            <div>  \n            \n            <button  className=\"Checkoutitems-button\" onClick={deleteBasket}>Delete</button> \n           </div>\n                 \n                 </div>\n                 </div>\n      \n    )\n}\n\nexport default CheckoutItem\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/CheckoutTotal.js",["105","106"],"import React from 'react'\nimport { useStateValue} from \"./StateProvider\";\n\n\nfunction CheckoutTotal({id, title, price}) {\n    const [{basket}, dispatch] = useStateValue();\n    return (\n        <div className=\"checkoutitems\">\n             \n           <div className=\"checkoutitems-info\">\n           \n               <p className=\"checkoutitems_title\">{title}</p>\n              \n                <p className=\"checkoutitems-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n               \n          \n                 </div>\n                 </div>\n      \n    )\n}\n\nexport default CheckoutTotal\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Login.js",["107","108","109"],"import React, {useState} from 'react';\nimport \"./Login.css\"\nimport {Link, useHistory} from \"react-router-dom\"\nimport { auth}  from \"./firebase\";\n\n\n\n\nfunction Login() {\n\n    const[email, setEmail] = useState('');\n    const[password, setPassword] = useState('');\n    const history = useHistory();\n\n    const signIn = event => {\n        event.preventDefault();\n\n        auth\n\n        .signInWithEmailAndPassword(email, password)\n        .then(auth => {\n            history.push('/')\n        })\n            .catch(error => alert(error.message))\n        \n        }\n\n        const register = event => {\n            event.preventDefault();\n            auth\n            .createUserWithEmailAndPassword(email, password)\n            .then((auth) => {\n                if (auth) {\n                history.push('/')\n                }\n            })\n                .catch(error => alert(error.message))\n         \n        }\n\n\n    return (\n        <div className=\"login\">\n            <Link to=\"/\">\n            <img className=\"login-logo\" src=\"images/bkologo.jpg\"  alt=\"logo picture\"/>\n            </Link>\n       \n        <div  className=\"login-container\" > \n        <h1> SignIn  </h1>\n        <form>\n        <a href=\"#\" className=\"fb-btn\">\n        <i className=\"fa fa-facebook fa-fw\"></i>  Facebook </a>\n          <br></br>\n          <hr></hr>\n          <br></br>\n          <h5> Email:</h5>\n        <input  value={email}  onChange={event => setEmail(event.target.value)} type=\"Email\" name=\"email\" placeholder=\"Email\" required/>\n        <h5> Password:</h5>\n        <input  value={password} onChange={event => setPassword(event.target.value)} type=\"password\" name=\"password\" placeholder=\"Password\" required/>\n        <button  type='submit' onClick={signIn} className=\"login-button\"> Sign In</button>\n       </form>\n       <p>By creating an account you agree to our <a href=\"#\" >Terms & Privacy</a>.</p>\n       <button  onClick={register} className=\"signup-button\" > create an new account</button>\n        </div>\n       \n        </div>\n    )\n}\n\nexport default Login\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Firebase.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/firebase.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Payment.js",["110"],"import React, {useState} from 'react'\nimport \"./Payment.css\"\nimport {useStateValue} from \"./StateProvider\";\nimport CheckoutItem from './CheckoutItem';\nimport PayPal from './PayPal';\nimport {Link} from \"react-router-dom\"\n\nfunction Payment() {\n    const [{basket, user}, dispatch] = useStateValue();\n    const [checkout, setCheckOut] = useState(false);\n    return (\n        <div className=\"payment\">\n         <div className=\"payment-container\">\n             <h1>\n                 Checkout(<Link to=\"/checkoutItem\">{basket?.length} items</Link>)\n             </h1>\n             </div>\n             <div className=\"payment-part\">\n             <div className=\"payment-header\">\n            <h4> Delivery Address</h4>\n            </div>\n            <div className=\"payment-