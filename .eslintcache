=\"product-info\">\n                <p>{title}</p>\n                <p className=\"product-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n                <div className=\"product-rating\">\n                {Array(rating).fill().map ((_, i) => (<p>⭐️</p> ))}\n                </div>\n            </div>\n                <img  className=\"product-image\" src={image}  alt=\"\"/>\n                <button onClick={addBasket}> Add to basket</button>\n            </div>\n       \n        \n    )\n}\n\nexport default Product\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Checkout.js",["87","88"],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/total.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Total.js",["89"],"import React from 'react';\nimport   \"./Total.css\";\nimport CurrencyFormat from \"react-currency-format\";\nimport{BasketItems} from \"./Reducer\";\nimport {useStateValue} from \"./StateProvider\";\nimport CheckoutTotal from './CheckoutTotal';\n\n\n\nfunction Total() {\n    const [{basket}, dispatch]= useStateValue();\n    \n    return (\n       <div>\n        <div className=\"subtotal\">\n           <CurrencyFormat\n          renderText={(value)=> (\n              <> \n              <p>\n                  detail({basket.length} items):\n         \n          {basket.map (item => (\n            <CheckoutTotal\n            id={item.id} \n            title= {item.title}\n            price= {item.price}\n            \n            />\n               ))}\n               <hr></hr>\n                <strong> Total:{value}</strong>\n              </p>\n              {/* <small className=\"subtotal-gift\">\n                  <input type=\"checkbox\"/>This order\n                  contains a gift\n              </small> */}\n             \n                  </>\n          )}\n          decimalScale={2}\n          value={BasketItems(basket)}\n          displayType={\"text\"}\n          thousandSeparator={true}\n          prefix={\"$\"}\n          /> \n          <button> Proceed to checkout</button>\n        </div>\n        </div>\n    )\n}\n\nexport default Total\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/StateProvider.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Reducer.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/CheckoutItem.js",["90"],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/CheckoutTotal.js",["91","92"],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Login.js",["93","94","95"],"import React, {useState} from 'react';\nimport \"./Login.css\"\nimport {Link, useHistory} from \"react-router-dom\"\nimport { auth}  from \"./firebase\";\n\n\n\n\nfunction Login() {\n\n    const[email, setEmail] = useState('');\n    const[password, setPassword] = useState('');\n    const history = useHistory();\n\n    const signIn = event => {\n        event.preventDefault();\n\n        auth\n\n        .signInWithEmailAndPassword(email, password)\n        .then(auth => {\n            history.push('/')\n        })\n            .catch(error => alert(error.message))\n        \n        }\n\n        const register = event => {\n            event.preventDefault();\n            auth\n            .createUserWithEmailAndPassword(email, password)\n            .then((auth) => {\n                if (auth) {\n                history.push('/')\n                }\n            })\n                .catch(error => alert(error.message))\n         \n        }\n\n\n    return (\n        <div className=\"login\">\n            <Link to=\"/\">\n            <img className=\"login-logo\" src=\"images/bkologo.jpg\"  alt=\"logo picture\"/>\n            </Link>\n       \n        <div  className=\"login-container\" > \n        <h1> SingIn  </h1>\n        <form>\n        <a href=\"#\" className=\"fb-btn\">\n        <i className=\"fa fa-facebook fa-fw\"></i>  Facebook </a>\n          <br></br>\n          <hr></hr>\n          <br></br>\n          <h5> Email:</h5>\n        <input  value={email}  onChange={event => setEmail(event.target.value)} type=\"Email\" name=\"email\" placeholder=\"Email\" required/>\n        <h5> Password:</h5>\n        <input  value={password} onChange={event => setPassword(event.target.value)} type=\"password\" name=\"password\" placeholder=\"Password\" required/>\n        <button  type='submit' onClick={signIn} className=\"login-button\"> Sign In</button>\n       </form>\n       <p>By creating an account you agree to our <a href=\"#\" >Terms & Privacy</a>.</p>\n       <button  onClick={register} className=\"signup-button\" > create an new account</button>\n        </div>\n       \n        </div>\n    )\n}\n\nexport default Login\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Firebase.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/firebase.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Payment.js",["96"],{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","severity":1,"message":"102","line":17,"column":9,"nodeType":"103","messageId":"104","endLine":17,"endColumn":11},{"ruleId":"105","severity":1,"message":"106","line":40,"column":6,"nodeType":"107","endLine":40,"endColumn":8,"suggestions":"108"},{"ruleId":"97","replacedBy":"109"},{"ruleId":"99","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":10,"column":26,"nodeType":"113","messageId":"114","endLine":10,"endColumn":34},{"ruleId":"97","replacedBy":"115"},{"ruleId":"99","replacedBy":"116"},{"ruleId":"111","severity":1,"message":"117","line":8,"column":14,"nodeType":"113","messageId":"114","endLine":8,"endColumn":20},{"ruleId":"111","severity":1,"message":"118","line":6,"column":8,"nodeType":"113","messageId":"114","endLine":6,"endColumn":15},{"ruleId":"111","severity":1,"message":"112","line":15,"column":22,"nodeTyp