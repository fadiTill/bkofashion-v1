/Switch>\n    </div>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/index.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Header.js",["129","130"]," import React from 'react'\n// import {useState} from 'react'\nimport './Header.css'\nimport './Productdetails.css'\n import  SearchIcon  from \"@material-ui/icons/Search\";\nimport ShoppingBasketIcon from '@material-ui/icons/ShoppingBasket';\nimport {Link} from \"react-router-dom\"\nimport {useStateValue} from \"./StateProvider\";\nimport { auth } from './firebase';\n\nfunction Header() {\n  //  const[searchProduct, setSearchProduct]= useState('')\n  const [{basket, user}, dispatch] = useStateValue();\n\n  const handleAuthenticaton = () => {\n    if (user){\n      auth.signOut();\n    }\n  }\n    return (\n        <div className=\"header\">\n          <Link to=\"/\" >\n          <img className=\"header_logo\"src=\"images/transparent.png\" alt=\"logo\"/> \n          </Link>\n         {/* <div className=\"header_search\">\n          <input className=\"header_searchInput\" type=\"text\" placeholder=\"Search...\"onChange={event =>{setSearchProduct(event.target.value)}}/>\n          <SearchIcon\n           className=\"header_searchIcon\" />\n        </div>  */}\n\n        <div className=\"header_nav\">\n          <Link to={!user &&\"/login\"}>\n        <div  onClick={handleAuthenticaton} className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Hello {!user? 'Guest' : user.email}</span>\n       <span\n        className=\"header_optionLineTwo\">{user? 'sign Out':'Sign In'}</span>\n       </div>\n       </Link>\n\n       <div className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Returns</span>\n       <span\n        className=\"header_optionLineTwo\">& Orders</span>\n       </div>\n\n       <div className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Your</span>\n       <span\n        className=\"header_optionLineTwo\">BKOPSales </span>\n       </div>\n       \n       <div className=\"header_nav\">\n       \n       <Link to=\"/productdetails\">\n       <div  className=\"header_option\">\n       <span\n        className=\"header_optionLineOne\">Product</span>\n       <span\n        className=\"header_optionLineTwo\">Details</span>\n        </div>\n        </Link>\n       </div>\n      \n      <Link to=\"/checkout\">\n      <div  className=\"header_optionCart\">\n      <ShoppingBasketIcon/>\n    <span className=\"header_optionLineTwo header_basketCount\">{basket?.length}</span>\n      </div>\n      </Link>\n   \n   </div>\n  </div>\n    )\n};\n\nexport default Header\n\n\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Home.js",["131"],"import {useState} from 'react'\n import ImgCarousel from './ImgCarousel'\nimport './Home.css'\n import Product from './Product'\nimport { CarouselData } from './CarouselData'\nimport {ProductData} from './ProductData';\nimport  SearchIcon  from \"@material-ui/icons/Search\";\n\n\nfunction Home() {\n\n  const[searchProduct, setSearchProduct]= useState('')\n    return (\n      \n\n        <div className=\"home\">\n            <div className=\"home-container\" >\n              {/* <img className=\"home-image\" src=\"images/bkotaie72.jpg\" alt=\"Red Pillow\"/>  */}\n              <ImgCarousel carousels={CarouselData}/> \n              <div className=\"header_search\">\n             <input className=\"header_searchInput\" type=\"text\" placeholder=\"Search...\" onChange={event =>{setSearchProduct(event.target.value)}}/>\n              <SearchIcon\n              className=\"header_searchIcon\" />\n              </div>  \n              <br>\n              </br>    \n              \n              <div className=\"home-row\">\n              {ProductData.filter((prod) =>{\n                if (searchProduct ===\"\"){\n                  return prod \n\n                } else if (prod.title.toLowerCase().includes(searchProduct.toLowerCase()))\n                return prod\n\n              })\n              .map((prod, index) => {\n                  return (\n                //   <img src={product.image}  className=\"image\"   alt=\" image\"/>\n                \n                <Product\n                id={prod.id}\n                title={prod.title}\n                image={prod.image} \n                price={prod.price} \n                rating={prod.rating}/>\n                  )\n              }\n              \n                 /* <Product \n                 id={1}\n                 title=\"Dark-Blue Mulberry Silk Pillowcase\" \n                 image=\"images/dark-blue.png\" \n                 price={44.99} \n                 rating={5}/>\n                 <Product \n                 id={2}\n                 title=\"Dark-Purple Mulberry Silk Pillowcase\"\n                 image=\"images/purple.png\"\n                 price={44.99} \n                 rating={5} />\n                 <Product  \n                 id={3}\n                 title=\"Coral-Red Mulberry Silk Pillowcase\"\n                 image=\"images/pink.png\"\n                 price={44.99} \n                 rating={5} />\n                  </div>\n            <div className=\"home-row\">\n                 <Product \n                 id={4}\n                 title=\"Turquoise Mulberry Silk Pillowcase\"\n                 image=\"images/turquoise.png\" \n                 price={44.99} \n                 rating={5}/>\n                 <Product \n                 id={5}\n                 title=\"Ivory Mulberry Silk Pillowcase\"\n                 image=\"images/ivory.png\"\n                 price={44.99} \n                 rating={5} />     \n                 <Product \n                 id={6}\n                 title=\"White-Ivory Mulberry Silk Pillowcase\"\n                 image=\"images/ivory-white.png\"\n                 price={44.99} \n                 rating={5} />    */\n                \n              )}\n            </div>\n        </div>\n        \n     </div>\n                \n    )\n}\n\nexport default Home\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Product.js",["132"],"\n\nimport React from 'react';\nimport './Product.css';\nimport {useStateValue} from \"./StateProvider\";\nimport Rating from \"./Rating.js\"\n\nfunction Product({id, title, image, price, rating}) {\n     const [{basket}, dispatch] = useStateValue();\n    //  console.log(\" my basket>>\", basket);\n\n    const addBasket = () => {\n        // dispatch action into the data layer\n        dispatch({\n            type: \"ADD_BASKET\",\n            item: {\n            id: id,\n            title: title,\n            price: price,\n            rating: rating,\n            image: image,\n            },\n        });\n    };\n    return (\n        <div className=\"product\">\n            <div className=\"product-info\">\n                <p>{title}</p>\n                <p className=\"product-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n                {/* <div className=\"product-rating\">\n                {Array(rating).fill().map ((_, i) => (<p>★</p> ))}\n                </div> */}\n                <Rating/>\n            </div>\n                <img  className=\"product-image\" src={image}  alt=\"\"/>\n                <button onClick={addBasket}> Add to basket</button>\n            </div>\n       \n        \n    )\n}\n\nexport default Product\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Checkout.js",["133","134"],"\n\nimport React from 'react';\nimport  \"./Checkout.css\";\nimport Total from \"./Total.js\";\nimport Payment from \"./Payment.js\";\nimport { useStateValue} from \"./StateProvider\";\n// import { BasketItems } from './Reducer';\n import CheckoutItem from './CheckoutItem';\nimport { useHistory } from 'react-router-dom';\n\n\nfunction Checkout() {\n    const History = useHistory();\n    const [{basket}, dispatch] = useStateValue();\n    return (\n        <div className=\"checkout\">\n            <div clasName=\"chechout-left\">\n           {/* <img className=\"checkout-img\"src=\"images/image-top.jpg\" alt=\"\"/>  */}\n        \n           {/* <div>  */}\n\n           \n               <h2 className=\"checkout-title\"> Your shoping basket</h2>\n\n\n               {basket.map (item => (\n            <CheckoutItem\n            id={item.id} \n            title= {item.title}\n            price= {item.price}\n            rating= {item.rating}\n            image={item.image}\n            />\n               ))}\n               {/* </div> */}\n        </div>\n        <div  onClick={event => History.push('/payment')}className=\"checkout-right\">\n            <Total/>\n\n        </div>\n    </div>\n\n        \n\n       \n    );\n}\n\nexport default Checkout\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/total.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Total.js",["135"],"import React from 'react';\nimport   \"./Total.css\";\nimport CurrencyFormat from \"react-currency-format\";\nimport{BasketItems} from \"./Reducer\";\nimport {useStateValue} from \"./StateProvider\";\nimport CheckoutTotal from './CheckoutTotal';\n\n\n\nfunction Total() {\n    const [{basket}, dispatch]= useStateValue();\n    \n    return (\n       <div>\n        <div className=\"subtotal\">\n           <CurrencyFormat\n          renderText={(value)=> (\n              <> \n              <p>\n                  detail({basket.length} items):\n         \n          {basket.map (item => (\n            <CheckoutTotal\n            id={item.id} \n            title= {item.title}\n            price= {item.price}\n            \n            />\n               ))}\n               <hr></hr>\n                <strong> Total:{value}</strong>\n              </p>\n              {/* <small className=\"subtotal-gift\">\n                  <input type=\"checkbox\"/>This order\n                  contains a gift\n              </small> */}\n             \n                  </>\n          )}\n          decimalScale={2}\n          value={BasketItems(basket)}\n          displayType={\"text\"}\n          thousandSeparator={true}\n          prefix={\"$\"}\n          /> \n          <button> Proceed to checkout</button>\n        </div>\n        </div>\n    )\n}\n\nexport default Total\n","/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/StateProvider.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/Reducer.js",[],"/Users/fadimatabathiebo/Desktop/bkofashion-v1/bkofashion-v1/src/CheckoutItem.js",["136"],"import React from 'react'\nimport { useStateValue} from \"./StateProvider\";\nimport \"./CheckoutItem.css\"\n\nfunction CheckoutItem({id, title, image, price, rating}) {\n    const [{basket}, dispatch] = useStateValue();\n\n    const deleteBasket =()=> {\n        dispatch({\n            type: 'DELETE_BASKET',\n            id: id,\n        })\n    }\n    \n    return (\n        <div className=\"checkoutitems\">\n             \n           <div className=\"checkoutitems-info\">\n           \n               <p className=\"checkoutitme_title\">{title}</p>\n               <img  className=\"checkoutitems-image\" src={image}  alt=\"\"/>\n\n\n                <p className=\"checkoutitems-price\" > \n                <small>$</small>\n                <strong>{price}</strong>\n                </p>\n                <div className=\"checkoutitems-rating\">\n                {Array(rating).fill().map ((_, i) => (<p>★</p> ))}\n                </div>\n           \n            <div>  \n            \n            <button  className=\"Checkoutitems-button\" onClick={deleteBasket}>Delete</button> \n           </div>\n                 \n                 </div>\n                 </div>\n      \n    )\n}\n\nexport default CheckoutItem\n","/Users/fadimatabathiebo/Desktop/bkofashi